{"ast":null,"code":"import \"core-js/modules/es.iterator.constructor.js\";\nimport \"core-js/modules/es.iterator.filter.js\";\nimport { Icon } from '@iconify/vue';\nexport default {\n  name: 'FriendSearch',\n  components: {\n    Icon\n  },\n  props: {\n    users: {\n      type: Array,\n      required: true\n    },\n    placeholder: {\n      type: String,\n      default: 'Search friends...'\n    },\n    actionText: {\n      type: String,\n      default: 'Add'\n    },\n    emptyText: {\n      type: String,\n      default: 'Start typing to search'\n    }\n  },\n  data() {\n    return {\n      isOpen: false,\n      searchQuery: ''\n    };\n  },\n  computed: {\n    filteredUsers() {\n      if (!this.searchQuery) {\n        return this.users.slice(0, 10);\n      }\n      const q = this.searchQuery.toLowerCase();\n      return this.users.filter(u => u.username.toLowerCase().includes(q));\n    }\n  },\n  methods: {\n    toggleOpen() {\n      this.isOpen = !this.isOpen;\n      this.$nextTick(() => {\n        this.$refs.searchInput?.focus();\n      });\n    },\n    closeDropdown() {\n      this.isOpen = false;\n      this.searchQuery = '';\n    },\n    selectUser(user) {\n      this.$emit('select', user);\n      this.closeDropdown();\n    }\n  }\n};","map":{"version":3,"names":["Icon","name","components","props","users","type","Array","required","placeholder","String","default","actionText","emptyText","data","isOpen","searchQuery","computed","filteredUsers","slice","q","toLowerCase","filter","u","username","includes","methods","toggleOpen","$nextTick","$refs","searchInput","focus","closeDropdown","selectUser","user","$emit"],"sources":["D:\\login-feed\\src\\components\\FriendSearch.vue"],"sourcesContent":["<template>\r\n  <!-- Search Icon Button -->\r\n  <div class=\"relative w-16\">\r\n    <button\r\n      @click=\"toggleOpen\"\r\n      class=\"rounded-xl hover:bg-gray-100 transition-all duration-200 group p-2\">\r\n      <Icon\r\n        icon=\"solar:magnifer-outline\"\r\n