iv>\r\n\r\n        <!-- Empty -->\r\n        <div\r\n          v-else\r\n          class=\"px-4 py-6 text-center text-gray-400\">\r\n          {{ emptyText }}\r\n        </div>\r\n      </div>\r\n    </Transition>\r\n  </div>\r\n\r\n  <!-- Click outside -->\r\n  <Teleport to=\"body\">\r\n    <div\r\n      v-if=\"isOpen\"\r\n      class=\"fixed inset-0 z-40\"\r\n      @click=\"closeDropdown\"\r\n    />\r\n  </Teleport>\r\n\r\n\r\n</template>\r\n\r\n\r\n<script>\r\nimport { Icon } from '@iconify/vue'\r\n\r\nexport default {\r\n  name: 'FriendSearch',\r\n  components: { Icon },\r\n\r\n  props: {\r\n    users: {\r\n      type: Array,\r\n      required: true\r\n    },\r\n    placeholder: {\r\n      type: String,\r\n      default: 'Search friends...'\r\n    },\r\n    actionText: {\r\n      type: String,\r\n      default: 'Add'\r\n    },\r\n    emptyText: {\r\n      type: String,\r\n      default: 'Start typing to search'\r\n    }\r\n  },\r\n\r\n  data() {\r\n    return {\r\n      isOpen: false,\r\n      searchQuery: ''\r\n    }\r\n  },\r\n\r\n  computed: {\r\n    filteredUsers() {\r\n      if (!this.searchQuery) {\r\n        return this.users.slice(0, 10)\r\n      }\r\n      const q = this.searchQuery.toLowerCase()\r\n      return this.users.filter(u =>\r\n        u.username.toLowerCase().includes(q)\r\n      )\r\n    }\r\n  },\r\n\r\n  methods: {\r\n    toggleOpen() {\r\n      this.isOpen = !this.isOpen\r\n      this.$nextTick(() => {\r\n        this.$refs.searchInput?.focus()\r\n      })\r\n    },\r\n\r\n    closeDropdown() {\r\n      this.isOpen = false\r\n      this.searchQuery = ''\r\n    },\r\n\r\n    selectUser(user) {\r\n      this.$emit('select', user)\r\n      this.closeDropdown()\r\n    }\r\n  }\r\n}\r\n</script>\r\n"],"mappings":";;AAuFA,SAASA,IAAG,QAAS,cAAa;AAElC,eAAe;EACbC,IAAI,EAAE,cAAc;EACpBC,UAAU,EAAE;IAAEF;EAAK,CAAC;EAEpBG,KAAK,EAAE;IACLC,KAAK,EAAE;MACLC,IAAI,EAAEC,KAAK;MACXC,QAAQ,EAAE;IACZ,CAAC;IACDC,WAAW,EAAE;MACXH,IAAI,EAAEI,MAAM;MACZC,OAAO,EAAE;IACX,