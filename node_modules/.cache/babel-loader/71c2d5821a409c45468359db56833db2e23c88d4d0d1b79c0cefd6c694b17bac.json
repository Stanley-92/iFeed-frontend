{"ast":null,"code":"import { Icon } from '@iconify/vue';\nimport avatar from '@/assets/mini1.jpg';\nexport default {\n  name: 'StoryReelPopup',\n  components: {\n    Icon\n  },\n  props: {\n    videoSrc: {\n      type: String,\n      required: true\n    }\n  },\n  emits: ['close'],\n  data() {\n    return {\n      avatar,\n      isPlaying: false // start paused - more user-friendly\n    };\n  },\n  mounted() {\n    // Optional: auto play after mount (many browsers block it)\n    this.$nextTick(() => {\n      this.togglePlay(); // first click will start it\n    });\n  },\n  methods: {\n    togglePlay() {\n      const video = this.$refs.videoRef;\n      if (!video) return;\n      if (video.paused || video.ended) {\n        video.play().then(() => {\n          this.isPlaying = true;\n        }).catch(err => console.warn(\"Play prevented:\", err));\n      } else {\n        video.pause();\n        this.isPlaying = false;\n      }\n    },\n    goToMainfeed() {\n      console.log('→ Going to main feed');\n      // this.$router.push('/feed') or similar\n    }\n  },\n  beforeUnmount() {\n    if (this.$refs.videoRef) {\n      this.$refs.videoRef.pause?.();\n    }\n  }\n};","map":{"version":3,"names":["Icon","avatar","name","components","props","videoSrc","type","String","required","emits","data","isPlaying","mounted","$nextTick","togglePlay","methods","video","$refs","videoRef","paused","ended","play","then","catch","err","console","warn","pause","goToMainfeed","log","beforeUnmount"],"sources":["D:\\login-feed\\src\\views\\Reel.vue"],"sourcesContent":["<template>\r\n  <div\r\n    class=\"fixed inset-0 z-50 flex items-center justify-center bg-black/80 backdrop-blur-sm\"\r\n    @click.self=\"$emit('close')\"\r\n  >\r\n    <div\r\n      class=\"relative w-full max-w-[420px] aspect-[9/16] bg-gray-900 rounded-3xl overflow-hidden shadow-2xl ring-1 ring-white/10\"\r\n      @click.stop\r\n    >\r\n      <!-- === VIDEO PLAYER === -->\r\n      <div class=\"absolute inset-0\">\r\n        <video\r\n          ref=\"videoRef\"\r\n          class=\"h-full w-full object-cover\"\r\n          playsinline\r\n          loop\r\n          muted\r\n          :src=\"videoSrc\"\r\n          @click=\"togglePlay\"\r\n          @play=\"isPlaying = true\"\r\n          @pause=\"isPlaying = false\"\r\n        ></video>\r\n\r\n        <!-- Gradient overlay -->\r\n        <div class=\"absolute inset-0 bg-gradient-to-b from-black/20 via-transparent to-black/70 pointer-events-none\" />\r\n      </div>\r\n\r\n      <!-- Top bar -->\r\n      <div class=\"absolute top-4 left-4 right-4 flex items-center justify-between z-30\">\r\n        <div class=\"flex-1 h-1 bg-white/30 rounded-full overflow-hidden\">\r\n          <div class=\"h-full w-3/5 bg-white rounded-full transition-all duration-[4000ms]\" />\r\n        </div>\r\n\r\n        <button\r\n          @click=\"$emit('close')\"\r\n          class=\"ml-3 p-2 rounded-full hover:bg-white/20 text-white transition-colors\"\r\n        >\r\n          <Icon icon=\"mdi:close\" class=\"w-7 h-7\" />\r\n        </button>\r\n      </div>\r\n\r\n      <!-- Bottom content -->\r\n      <div class=\"absolute bottom-0 left-0 right-0 p-5 pb-8\">\r\n        <div class=\"flex items-center justify-between mb-4\">\r\n          <div class=\"flex items-center gap-3\">\r\n            <div class=\"w-10 h-10 rounded-full ring-2 ring-white/40 overflow-hidden\">\r\n              <img :src=\"avatar\" alt=\"avatar\" class=\"w-full h-full object-cover\" />\r\n            </div>\r\n            <div class=\"text-white\">\r\n              <p class=\"font-semibold\">Jena</p>\r\n              <p class=\"text-xs text-white/70\">3h ago</p>\r\n            </div>\r\n          </div>\r\n\r\n          <button class=\"px-5 py-1.5 bg-white text-black text-sm font-medium rounded-full hover:bg-gray-200 transition\">\r\n            Follow\r\n          </button>\r\n        </div>\r\n\r\n        <p class=\"text-white text-sm leading-relaxed\">\r\n          This is my little video ♡\r\n        </p>\r\n      </div>\r\n\r\n      <!-- === RIGHT SIDE ACTION COLUMN === -->\r\n      <div class=\"absolute right-5 top-1/2 -translate-y-1/2 flex flex-col items-center gap-7 z-20\">\r\n        <button class=\"group\">\r\n          <div class=\"w-12 h-12 flex items-center justify-center rounded-full bg-black/40 backdrop-blur-lg shadow-lg group-hover:scale-110 transition-transform\">\r\n            <Icon icon=\"solar:heart-bold\" class=\"w-6 h-6 text-white group-hover:text-pink-400\" />\r\n          </div>\r\n        </button>\r\n\r\n        <button class=\"group\">\r\n          <div class=\"w-12 h-12 flex items-center justify-center rounded-full bg-black/40 backdrop-blur-lg shadow-lg group-hover:scale-110 transition-transform\">\r\n            <Icon icon=\"solar:chat-line-bold\" class=\"w-6 h-6 text-white group-hover:text-blue-400\" />\r\n          </div>\r\n        </button>\r\n\r\n        <button class=\"group\">\r\n          <div class=\"w-12 h-12 flex items-center justify-center rounded-full bg-black/40 backdrop-blur-lg shadow-lg group-hover:scale-110 transition-transform\">\r\n            <Icon icon=\"solar:share-bold\" class=\"w-6 h-6 text-white group-hover:text-green-400\" />\r\n          </div>\r\n        </button>\r\n\r\n        <button class=\"group\">\r\n          <div class=\"w-12 h-12 flex items-center justify-center rounded-full bg-black/40 backdrop-blur-lg shadow-lg group-hover:scale-110 transition-transform\">\r\n            <Icon icon=\"solar:bookmark-bold\" class=\"w-6 h-6 text-white group-hover:text-yellow-400\" />\r\n          </div>\r\n        </button>\r\n      </div>\r\n\r\n      <!-- === VIDEO CONTROLS (bottom right) === -->\r\n      <div class=\"absolute bottom-24 right-6 z-30 flex flex-col gap-4\">\r\n        <button\r\n          @click=\"togglePlay\"\r\n          class=\"w-11 h-11 flex items-center justify-center rounded-full bg-black/50 backdrop-blur-md text-white hover:bg-black/70 transition shadow-lg\"\r\n        >\r\n          <Icon :icon=\"isPlaying ? 'solar:pause-bold' : 'solar:play-bold'\" class=\"w-6 h-6\" />\r\n        </button>\r\n      </div>\r\n\r\n      <!-- Back to feed button -->\r\n      <button\r\n        @click=\"goToMainfeed\"\r\n        class=\"absolute top-6 left-6 p-3 rounded-xl bg-green-600 hover:bg-green-700 border-4 border-green-500 hover:border-green-400 text-white transition-all duration-200 z-30\"\r\n      >\r\n        <Icon icon=\"tdesign:chat-bubble\" class=\"w-9 h-9\" />\r\n      </button>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { Icon } from '@iconify/vue'\r\nimport avatar from '@/assets/mini1.jpg'\r\n\r\nexport default {\r\n  name: 'StoryReelPopup',\r\n\r\n  components: { Icon },\r\n\r\n  props: {\r\n    videoSrc: {\r\n      type: String,\r\n      required: true\r\n    }\r\n  },\r\n\r\n  emits: ['close'],\r\n\r\n  data() {\r\n    return {\r\n      avatar,\r\n      isPlaying: false   // start paused - more user-friendly\r\n    }\r\n  },\r\n\r\n  mounted() {\r\n    // Optional: auto play after mount (many browsers block it)\r\n    this.$nextTick(() => {\r\n      this.togglePlay()  // first click will start it\r\n    })\r\n  },\r\n\r\n  methods: {\r\n    togglePlay() {\r\n      const video = this.$refs.videoRef\r\n      if (!video) return\r\n\r\n      if (video.paused || video.ended) {\r\n        video.play()\r\n          .then(() => { this.isPlaying = true })\r\n          .catch(err => console.warn(\"Play prevented:\", err))\r\n      } else {\r\n        video.pause()\r\n        this.isPlaying = false\r\n      }\r\n    },\r\n\r\n    goToMainfeed() {\r\n      console.log('→ Going to main feed')\r\n      // this.$router.push('/feed') or similar\r\n    }\r\n  },\r\n\r\n  beforeUnmount() {\r\n    if (this.$refs.videoRef) {\r\n      this.$refs.videoRef.pause?.()\r\n    }\r\n  }\r\n}\r\n</script>"],"mappings":"AAiHA,SAASA,IAAG,QAAS,cAAa;AAClC,OAAOC,MAAK,MAAO,oBAAmB;AAEtC,eAAe;EACbC,IAAI,EAAE,gBAAgB;EAEtBC,UAAU,EAAE;IAAEH;EAAK,CAAC;