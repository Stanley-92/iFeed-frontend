{"ast":null,"code":"import { Icon } from '@iconify/vue';\nimport avatar from '@/assets/mini1.jpg';\nexport default {\n  name: 'StoryReelPopup',\n  components: {\n    Icon\n  },\n  props: {\n    videoSrc: {\n      type: String,\n      required: true\n    }\n  },\n  emits: ['close'],\n  data() {\n    return {\n      avatar,\n      isPlaying: false // start paused - more user-friendly\n    };\n  },\n  mounted() {\n    // Optional: auto play after mount (many browsers block it)\n    this.$nextTick(() => {\n      this.togglePlay(); // first click will start it\n    });\n  },\n  methods: {\n    togglePlay() {\n      const video = this.$refs.videoRef;\n      if (!video) return;\n      if (video.paused || video.ended) {\n        video.play().then(() => {\n          this.isPlaying = true;\n        }).catch(err => console.warn(\"Play prevented:\", err));\n      } else {\n        video.pause();\n        this.isPlaying = false;\n      }\n    },\n    goToMainfeed() {\n      console.log('→ Going to main feed');\n      // this.$router.push('/feed') or similar\n    }\n  },\n  beforeUnmount() {\n    if (this.$refs.videoRef) {\n      this.$refs.videoRef.pause?.();\n    }\n  }\n};","map":{"version":3,"names":["Icon","avatar","name","components","props","videoSrc","type","String","required","emits","data","isPlaying","mounted","$nextTick","togglePlay","methods","video","$refs","videoRef","paused","ended","play","then","catch","err","console","warn","pause","goToMainfeed","log","beforeUnmount"],"sources":["D:\\login-feed\\src\\views\\Reel.vue"],"sourcesContent":["<template>\r\n  <div\r\n    class=\"fixed inset-0 z-50 flex items-center justify-center bg-black/65 backdrop-blur-sm\"\r\n    @click.self=\"$emit('close')\">\r\n\r\n\r\n    <!-- Main story container -->\r\n    <div\r\n      class=\"relative w-full max-w-[420px] aspect-[9/16] bg-gray-900 rounded-3xl \r\n      overflow-hidden shadow-2xl ring-1 ring-white/10\"\r\n      @click.stop>\r\n\r\n\r\n      <!-- Background media -->\r\n      <div class=\"absolute inset-0\">\r\n        <img\r\n          :src=\"mediaSrc\"\r\n          alt=\"story media\"\r\n          class=\"h-full w-full object-cover\"/>\r\n\r\n\r\n        <!-- Subtle vignette + bottom gradient -->\r\n        <div class=\"absolute inset-0 bg-gradient-to-b from-black/20 \r\n        via-transparent to-black/70\" />\r\n      </div>\r\n\r\n\r\n<!-- Optional: Play/Pause + Sound toggle buttons -->\r\n<div class=\"absolute bottom-2 right-6 z-20 flex flex-col gap-4\">\r\n  <button \r\n    @click=\"toggleMute\" \r\n    class=\"w-10 h-10 flex items-center justify-center rounded-full bg-black/40 backdrop-blur-md text-white\">\r\n    <Icon :icon=\"isMuted ? 'solar:volume-cross-bold' : 'solar:volume-loud-bold'\" class=\"w-5 h-5\" />\r\n  </button>\r\n</div>\r\n\r\n\r\n\r\n      <!-- Top bar -->\r\n      <div class=\"absolute top-4 left-4 right-4 flex items-center justify-between z-20\">\r\n\r\n        <!-- Progress bar -->\r\n        <div class=\"flex-1 h-1 bg-white/30 rounded-full overflow-hidden\">\r\n          <div class=\"h-full w-[35%] bg-white rounded-full transition-all duration-500\" />\r\n        </div>\r\n\r\n        \r\n\r\n        <button\r\n          @click=\"$emit('close')\"\r\n          class=\"ml-4 p-4 -mr-4 rounded-full hover:bg-white/15 \r\n          text-white/90 hover:text-white transition-colors\">\r\n          <Icon icon=\"mdi:close\" class=\"w-6 h-6\" />\r\n\r\n          <svg class=\"w-6 h-6\" fill=\"none\" \r\n          stroke=\"currentColor\" \r\n          viewBox=\"0 0 24 24\">\r\n            <path stroke-linecap=\"round\" \r\n            stroke-linejoin=\"round\" stroke-width=\"2.2\" d=\"M6 \" />\r\n          </svg>\r\n        </button>\r\n        \r\n      </div>\r\n\r\n\r\n\r\n\r\n\r\n<!-- Replace the <img> section with this video player -->\r\n<div class=\"absolute inset-0\">\r\n  <!-- Video player -->\r\n  <video\r\n    ref=\"videoPlayer\"\r\n    class=\"h-full w-full object-cover\"\r\n    playsinline\r\n    autoplay\r\n    muted\r\n    loop\r\n    :src=\"videoSrc\"\r\n    poster=\"https://images.unsplash.com/photo-... (fallback thumbnail)\"\r\n>\r\n</video>\r\n\r\n  <!-- Optional overlay gradient -->\r\n  <div class=\"absolute inset-0 bg-gradient-to-b from-black/20 via-transparent to-black/70 pointer-events-none\" />\r\n</div>\r\n\r\n\r\n\r\n\r\n\r\n\r\n\r\n\r\n\r\n\r\n\r\n\r\n\r\n\r\n\r\n\r\n\r\n\r\n\r\n\r\n\r\n\r\n\r\n\r\n      \r\n\r\n      <!-- Bottom content -->\r\n      <div class=\"absolute bottom-0 left-0 right-0 p-5 pb-7\">\r\n        <!-- Username & follow -->\r\n        <div class=\"flex items-center justify-between mb-5\">\r\n          <div class=\"flex items-center gap-3\">\r\n            <div class=\"w-11 h-11 rounded-full ring-2 ring-white/40 overflow-hidden\">\r\n              <img\r\n                :src=\"avatar\"\r\n                alt=\"User avatar\"\r\n                class=\"w-full h-full object-cover\"\r\n              />\r\n            </div>\r\n            <div class=\"text-white\">\r\n              <p class=\"font-semibold tracking-tight mr-4\">Jena</p>\r\n              <p class=\"text-xs text-white/70\">3h ago</p>\r\n            </div>\r\n          </div>\r\n\r\n         \r\n            <button\r\n              class=\"px-4 py4 mr-80 \r\n              text-white text-sm font-semibold hover:text-blue-400\r\n              shadow-md transition-colors\">\r\n              Follow\r\n            </button>\r\n        </div>\r\n\r\n\r\n    \r\n      \r\n\r\n        <!-- Caption -->\r\n        <p class=\"text-white text-sm \">\r\n          This is my little video <br />\r\n        \r\n        </p>\r\n      </div>\r\n    </div>\r\n\r\n<!-- Quick actions -->\r\n<div class=\"flex right-5 top-1/3 flex flex-col items-center gap-4 text-white z-10 mt-80\">\r\n  <button class=\"group\">\r\n    <div class=\"w-11 h-11 flex items-center justify-center\r\n     rounded-full bg-black/50 backdrop-blur-md \r\n     hover:bg-black/70 transition-all shadow-md\">\r\n    <Icon icon=\"solar:heart-linear\"\r\n     class=\"w-6 h-6 group-hover:text-pink-400 transition-colors\" />\r\n    </div>\r\n  </button>\r\n\r\n  <button class=\"group\">\r\n    <div class=\"w-11 h-11 flex\r\n     items-center justify-center \r\n     rounded-full bg-black/50 backdrop-blur-md\r\n      hover:bg-black/70 transition-all shadow-md\">\r\n    <Icon icon=\"uil:comment\" class=\"w-6 h-6 group-hover:text-blue-400 transition-colors\" />\r\n    </div>\r\n  </button>\r\n\r\n  <button class=\"group\">\r\n    <div class=\"w-11 h-11 flex items-center justify-center rounded-full bg-black/50 backdrop-blur-md hover:bg-black/70 transition-all shadow-md\">\r\n      <Icon icon=\"eva:paper-plane-outline\" class=\"w-6 h-6 group-hover:text-green-400 transition-colors\" />\r\n    </div>\r\n\r\n  </button>\r\n   <button class=\"group\">\r\n    <div class=\"w-11 h-11 flex items-center justify-center rounded-full bg-black/50 backdrop-blur-md hover:bg-black/70 transition-all shadow-md\">\r\n      <Icon icon=\"stash:save-ribbon-light\" class=\"w-6 h-6 group-hover:text-green-400 transition-colors\" />\r\n    </div>\r\n  </button>\r\n</div>\r\n\r\n\r\n\r\n    <!-- Back to feed button (top-left corner) -->\r\n    <button\r\n      @click=\"goToMainfeed\"\r\n      class=\"absolute top-6 left-6 p-3 rounded-xl hover:bg-white/10 transition-all duration-200 group z-30\"\r\n    >\r\n      <Icon\r\n        icon=\"tdesign:chat-bubble\"\r\n        class=\"w-10 h-10 text-white \r\n        transition-colors bg-green-600 \r\n        rounded-xl border-4 border-green-500\r\n         group-hover:border-green-400 duration-200\"\r\n      />\r\n    </button>\r\n\r\n\r\n\r\n<!-- Place this inside the main container, absolute positioned on the right -->\r\n<div class=\"absolute right-4 top-1/3 flex flex-col items-center gap-2 text-white z-10\">\r\n  <button class=\"flex flex-col items-center gap-1.5 group\">\r\n    <div class=\"w-12 h-12 flex items-center justify-center rounded-full \r\n    bg-black/20 backdrop-blur-lg\r\n     shadow-lg group-hover:scale-110 transition-transform duration-200\">\r\n      <Icon icon=\"mingcute:up-line\" class=\"w-10 h-10\" />\r\n    </div>\r\n  </button>\r\n\r\n  <button class=\"flex flex-col items-center gap-1.5 group\">\r\n    <div class=\"w-12 h-12 flex items-center justify-center rounded-full \r\n    bg-gray/20 backdrop-blur-lg \r\n    shadow-lg group-hover:scale-110 transition-transform duration-200\">\r\n      <Icon icon=\"mingcute:down-line\" class=\"w-10 h-10\" />\r\n    </div>\r\n  </button>\r\n\r\n\r\n\r\n<!-- === VIDEO CONTROLS (bottom right) === -->\r\n      <div class=\"absolute bottom-24 right-6 z-30 flex flex-col gap-4\">\r\n        <button\r\n          @click=\"togglePlay\"\r\n          class=\"w-11 h-11 flex items-center justify-center rounded-full bg-black/50 backdrop-blur-md text-white hover:bg-black/70 transition shadow-lg\"\r\n        >\r\n          <Icon :icon=\"isPlaying ? 'solar:pause-bold' : 'solar:play-bold'\" class=\"w-6 h-6\" />\r\n        </button>\r\n      </div>\r\n\r\n      <!-- Back to feed button -->\r\n      <button\r\n        @click=\"goToMainfeed\"\r\n        class=\"absolute top-6 left-6 p-3 rounded-xl bg-green-600 hover:bg-green-700 border-4 border-green-500 hover:border-green-400 text-white transition-all duration-200 z-30\"\r\n      >\r\n        <Icon icon=\"tdesign:chat-bubble\" class=\"w-9 h-9\" />\r\n      </button>\r\n</div>\r\n\r\n\r\n\r\n  </div>\r\n</template>\r\n<script>\r\nimport { Icon } from '@iconify/vue'\r\nimport avatar from '@/assets/mini1.jpg'\r\n\r\nexport default {\r\n  name: 'StoryReelPopup',\r\n\r\n  components: { Icon },\r\n\r\n  props: {\r\n    videoSrc: {\r\n      type: String,\r\n      required: true\r\n    }\r\n  },\r\n\r\n  emits: ['close'],\r\n\r\n  data() {\r\n    return {\r\n      avatar,\r\n      isPlaying: false   // start paused - more user-friendly\r\n    }\r\n  },\r\n\r\n  mounted() {\r\n    // Optional: auto play after mount (many browsers block it)\r\n    this.$nextTick(() => {\r\n      this.togglePlay()  // first click will start it\r\n    })\r\n  },\r\n\r\n  methods: {\r\n    togglePlay() {\r\n      const video = this.$refs.videoRef\r\n      if (!video) return\r\n\r\n      if (video.paused || video.ended) {\r\n        video.play()\r\n          .then(() => { this.isPlaying = true })\r\n          .catch(err => console.warn(\"Play prevented:\", err))\r\n      } else {\r\n        video.pause()\r\n        this.isPlaying = false\r\n      }\r\n    },\r\n\r\n    goToMainfeed() {\r\n      console.log('→ Going to main feed')\r\n      // this.$router.push('/feed') or similar\r\n    }\r\n  },\r\n\r\n  beforeUnmount() {\r\n    if (this.$refs.videoRef) {\r\n      this.$refs.videoRef.pause?.()\r\n    }\r\n  }\r\n}\r\n</script>"],"mappings":"AAsPA,SAASA,IAAG,QAAS,cAAa;AAClC,OAAOC,MAAK,MAAO,oBAAmB;AAEtC,eAAe;EACbC,IAAI,EAAE,gBAAgB;EAEtBC,UAAU,EAAE;IAAEH;EAAK,CAAC;EAEpBI,KAAK,EAAE;IACLC,QAAQ,EAAE;MACRC,IAAI,EAAEC,MAAM;MACZC,QAAQ,EAAE;IACZ;EACF,CAAC;EAEDC,KAAK,EAAE,CAAC,OAAO,CAAC;EAEhBC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLT,MAAM;MACNU,SAAS,EAAE,KAAI,CAAI;IACrB;EACF,CAAC;EAEDC,OAAOA,CAAA,EAAG;IACR;IACA,IAAI,CAACC,SAAS,CAAC,MAAM;MACnB,IAAI,CAACC,UAAU,CAAC,GAAG;IACrB,CAAC;EACH,CAAC;EAEDC,OAAO,EAAE;IACPD,UAAUA,CAAA,EAAG;MACX,MAAME,KAAI,GAAI,IAAI,CAACC,KAAK,CAACC,QAAO;MAChC,IAAI,CAACF,KAAK,EAAE;MAEZ,IAAIA,KAAK,CAACG,MAAK,IAAKH,KAAK,CAACI,KAAK,EAAE;QAC/BJ,KAAK,CAACK,IAAI,CAAC,EACRC,IAAI,CAAC,MAAM;UAAE,IAAI,CAACX,SAAQ,GAAI,IAAG;QAAE,CAAC,EACpCY,KAAK,CAACC,GAAE,IAAKC,OAAO,CAACC,IAAI,CAAC,iBAAiB,EAAEF,GAAG,CAAC;MACtD,OAAO;QACLR,KAAK,CAACW,KAAK,CAAC;QACZ,IAAI,CAAChB,SAAQ,GAAI,KAAI;MACvB;IACF,CAAC;IAEDiB,YAAYA,CAAA,EAAG;MACbH,OAAO,CAACI,GAAG,CAAC,sBAAsB;MAClC;IACF;EACF,CAAC;EAEDC,aAAaA,CAAA,EAAG;IACd,IAAI,IAAI,CAACb,KAAK,CAACC,QAAQ,EAAE;MACvB,IAAI,CAACD,KAAK,CAACC,QAAQ,CAACS,KAAK,GAAG;IAC9B;EACF;AACF","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}